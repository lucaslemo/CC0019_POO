package br.edu.ufca.basicas;

public class Celeiro {
	private int numAnimais;
	private String tipoAnimal;
	
	public Celeiro(int numAnimais, String tipoAnimal) {
		this.setAnimais(numAnimais);
		this.tipoAnimal = tipoAnimal;
	}

	public int getAnimais() {
		return this.numAnimais;
	}

	public void setAnimais(int numAnimais) {
		this.numAnimais = numAnimais;
	}

	public String getTipoAnimal() {
		return tipoAnimal;
	}
		
}
